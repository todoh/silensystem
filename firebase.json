{
  "hosting": {
    "public": ".", 
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
       
      {
        "source": "/__/firebase/init.json",
        "destination": "/__/firebase/init.json"
      }
    ]
  }
}